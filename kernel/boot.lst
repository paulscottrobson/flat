              	;
              	;       Compile a reference to self.
              	;
              	; ***************************************************************************************
              	
8B7A:         	COMPCompileSelf:
8B7A: E3      		ex   (sp),hl        ; HL is the address, old HL on top of stack
8B7B: 3ECD    		ld  a,$CD         ; call opcode
8B7D: CD7D86  		call  FARCompileByte       ; compile that
8B80: CD9586  		call  FARCompileWord       ; compile the address
8B83: E1      		pop  hl          ; restore old HL
8B84: C9      		ret
              	
              	; ***************************************************************************************
              	;
              	;         Compile Macro Code
              	;
              	; ***************************************************************************************
              	
8B85:         	COMPMacroExpand:
8B85: E3      		ex   (sp),hl        ; HL is the address, old HL on top of stack
8B86: 23      		inc  hl          ; skip over the LD B,xx opcode
8B87: 46      		ld   b,(hl)         ; read count
8B88:         	__COMPMExLoop:
8B88: 23      		inc  hl          ; get next byte to copy
8B89: 7E      		ld   a,(hl)
8B8A: CD7D86  		call  FARCompileByte       ; compile that
8B8D: 10F9    		djnz  __COMPMExLoop      ; do it B times.
8B8F: E1      		pop  hl          ; restore old HL
8B90: C9      		ret
              	
              	
              	
